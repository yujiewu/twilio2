<header class="navbar navbar-fixed-top navbar-custom">
      <%= link_to "InstaPhodfadsfto", root_path, id: "logo" %>
      <nav>
        <ul class="nav pull-right">
          <li><%= link_to "Home", root_path, id: "links-nav" %></li>
          <li><%= link_to "Help", help_path, id: "links-nav" %></li>
          <% if signed_in? %>
              <li><%= link_to "Users", users_path, id: "links-nav" %></li>
              <li id="fat-menu" class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                  Account <b class="caret"></b>
                </a>
                <ul class="dropdown-menu">
                  <li><%= link_to "Profile", current_user, id: "links-nav" %></li>
                  <li><%= link_to "Settings", edit_user_path(current_user, id: "links-nav") %></li>
                  <li class="divider"></li>
                  <li>
                    <%= link_to "Sign out", signout_path, method: "delete", id: "links-nav" %>
                  </li>
                </ul>
              </li>
          <% else %>
              <li><%= link_to "Sign in", signin_path, id: "links-nav" %></li>
          <% end %>
        </ul>
      </nav>
</header>